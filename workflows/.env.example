# LLM API Keys for Chat Interface
# At least one of these is required for the chat functionality

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google API Key
GOOGLE_API_KEY=your_google_api_key_here

# Browser Configuration (optional)
# Set to true to run browser in headless mode by default
BROWSER_HEADLESS=false

# Set to false to enable browser security features
BROWSER_DISABLE_SECURITY=true

